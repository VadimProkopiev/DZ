{
    "razor.languageServer.debug": true,
    "razor.devmode": true,
    "razor.format.enable": true,
    "razor.trace": "Verbose",
    "csharp.suppressProjectJsonWarning": true,
    "omnisharp.analyzeOpenDocumentsOnly": true,
    "omnisharp.disableMSBuildDiagnosticWarning": true,
